{
  "name": "<%= moduleName %>",
  "version": "<%= moduleVersion %>",
  "description": "<%= moduleDesc %>",
  "main": "<%= buildDirectory %>/index.js",
  "scripts": {
    "start": "npx nodemon --exec babel-node <%= buildDirectory %>/index.js",
    "start:babel": "npx babel-node src/index.js",
    "test": "npx mocha --ui tdd --compilers js:@babel/register '<%= testDirectory %>/**/*.test.js'",
    "tdd": "npm test -- --watch",
    "build": "npx babel src -d <%= buildDirectory %>",
    "dev": "npx nodemon --watch src --exec babel-node src/index.js"
  },
  "author": "<%= name %> <<%= email %>>",
  "license": "<%= moduleLicense %>",
  "dependencies": {
    "winston": "2.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/register": "^7.7.4",
    "chai": "^4.2.0",
    "coveralls": "^2.13.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0",
    "lint-staged": "^4.2.1",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.1",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  }
}
