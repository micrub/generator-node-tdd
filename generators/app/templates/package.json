{
  "name": "<%= moduleName %>",
  "version": "<%= moduleVersion %>",
  "description": "<%= moduleDesc %>",
  "main": "<%= buildDirectory %>/index.js",
  "scripts": {
    "start": "node <%= buildDirectory %>/index.js",
    "start-babel": "babel-node src/index.js",
    "test":
      "mocha --require babel-core/register '<%= testDirectory %>/**/*.test.js'",
    "tdd": "npm test -- --watch",
    "build":
      "babel src -d <%= buildDirectory %> && cat <%= buildDirectory %>/index.js | nexe",
    "dev": "nodemon --watch src --exec babel-node src/index.js"
  },
  "keywords": [],
  "author": "<%= name %> <<%= email %>> (<%= website %>)",
  "license": "<%= moduleLicense %>",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "nexe": "^2.0.0-rc.20",
    "nodemon": "^1.12.1",
    "mocha": "^4.0.1"
  }
}
