  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/register": "^7.7.4",
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-strict": "^14.0.1",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^25.0.0",
    "jest-cli": "^25.0.0",
    "lint-staged": "^4.2.1",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.1",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^2.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "if-empty": "^1.0.0",
    "mkdirp": "^0.5.1",
    "yeoman-generator": "^4.2.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "echo TODO!!!",
    "pretest": "npx eslint ./generators/",
    "test": "npx mocha __tests__/"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "ulisesantana/generator-node-tdd",
  "license": "MIT"
}
